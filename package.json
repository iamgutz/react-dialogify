{
  "name": "react-global-dialog",
  "version": "1.0.0",
  "description": "Simple, Fast and Easy to use Dialog Modal component for React",
  "main": "dist/index.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "install": "^0.10.1",
    "mocha": "^5.2.0",
    "npm": "^6.4.1",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "prerelease": "git add --all && git commit -m\"Publishing update\"",
    "push-master": "git push origin master",
    "release": "npm version patch && npm run push-master && npm publish",
    "prepublish": "npm run build",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel src --out-dir dist --ignore test.js",
    "clean-dist": "rimraf ./dist",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "mocha src/*.test.js --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamgutz/react-global-dialog.git"
  },
  "keywords": [
    "React",
    "Dialog",
    "Modal",
    "Simple",
    "Customizable",
    "Fast",
    "Easy",
    "Programmatically controlled"
  ],
  "author": "Michael Gutierrez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamgutz/react-global-dialog/issues"
  },
  "homepage": "https://github.com/iamgutz/react-global-dialog#readme",
  "dependencies": {
    "react": "^16.6.3",
    "web-redux-socket": "^1.0.11"
  }
}
